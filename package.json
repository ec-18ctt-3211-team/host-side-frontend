{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-bugfix-v8-spread-parameters-in-optional-chaining": "^7.14.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "autoprefixer": "10.x",
    "axios": "^0.21.1",
    "cross-env": "7.x",
    "npm": "^7.20.5",
    "npm-run-all": "4.x",
    "postcss": "8.x",
    "postcss-cli": "8.x",
    "postcss-preset-env": "6.x",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "tailwindcss": "^2.2.2",
    "typescript": "4.x"
  },
  "scripts": {
    "start": "BROWSER=none run-p watch:css react-scripts:start",
    "build": "run-s build:css react-scripts:build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "cross-env TAILWIND_MODE=build NODE_ENV=production postcss src/styles/global.css -o src/styles/index.css",
    "watch:css": "cross-env TAILWIND_MODE=watch NODE_ENV=development postcss src/styles/global.css -o src/styles/index.css --watch",
    "react-scripts:start": "react-scripts start",
    "react-scripts:build": "react-scripts build",
    "lint": "eslint --fix .",
    "format": "prettier --write .",
    "precommit": "prettier --write . && eslint --fix ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@iconify/icons-teenyicons": "^1.1.0",
    "@iconify/react": "^1.1.4",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
